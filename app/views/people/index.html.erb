<h1 class="center">Your people!</h1>
<%= render 'layouts/navbar' %>
<%= yield %>
<hr>
<h3 class="center"><%= link_to 'Create New Entry', new_person_path %></h3>

 <% @people.each do |person| %>
 <h2><%= person.first_name + " " + person.last_name %></h3>
 <%= link_to 'View', person_path(person) %>
 <%= link_to 'Edit', edit_person_path(person) %>
 <%= link_to 'Delete', person, method: :delete %>
 <% end %>
